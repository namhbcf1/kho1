# Environment variables for KhoAugment POS Frontend

# API Configuration
VITE_API_URL=http://localhost:8787
VITE_APP_ENV=development

# Cloudflare Configuration
VITE_CLOUDFLARE_ACCOUNT_ID=your-cloudflare-account-id

# Payment Gateway Configuration
VITE_VNPAY_MERCHANT_ID=your-vnpay-merchant-id
VITE_MOMO_PARTNER_CODE=your-momo-partner-code
VITE_ZALOPAY_APP_ID=your-zalopay-app-id

# Analytics and Monitoring
VITE_SENTRY_DSN=your-sentry-dsn
VITE_GOOGLE_ANALYTICS_ID=your-google-analytics-id

# PWA Configuration
VITE_VAPID_PUBLIC_KEY=your-vapid-public-key

# Feature Flags
VITE_ENABLE_OFFLINE_MODE=true
VITE_ENABLE_PUSH_NOTIFICATIONS=true
VITE_ENABLE_ANALYTICS=true

# Development Configuration
VITE_DEBUG_MODE=false
VITE_MOCK_API=false
